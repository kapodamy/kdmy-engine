{
    "configurations": [
        {
            "name": "WASM",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/include",
                "${env:EMSDK}/upstream/emscripten/cache/sysroot/include"
            ],
            "defines": [
                "UNICODE"
            ],
            "windowsSdkVersion": "8.1",
            "compilerPath": "${env:EMSDK}\\upstream\\emscripten\\emcc.bat",//"C:/msys64/mingw64/bin/gcc.exe",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-clang-x86",
            "configurationProvider": "ms-vscode.makefile-tools",
            "compilerArgs": [
                "-Wall",
                "-Wextra",
                "-Wfloat-conversion"
            ]
        }
    ],
    "version": 4
}