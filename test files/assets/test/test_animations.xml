<?xml version="1.0" encoding="utf-8"?>
<AnimationList frameRate="24">
    <AnimationMacro name="on">
        <Interpolator type="linear" property="skewX" start="0.0" end="-0.1" duration="1000" />
        <Interpolator type="linear" property="skewY" start="0.0" end="-1.5" duration="1000" />
        <Yield />
        <Interpolator type="linear" property="skewX" start="-0.1" end="0" duration="1000" />
        <Interpolator type="linear" property="skewY" start="-1.5" end="0" duration="1000" />
        <Yield />
    </AnimationMacro>
    <AnimationMacro name="off">
        <Set property="skewX" value="0.0" />
        <Set property="skewY" value="0.0" />
    </AnimationMacro>
</AnimationList>
