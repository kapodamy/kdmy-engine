{
    "name": "PICO",
    "model": "Pico_FNF_assetss.xml",
    "weekSelectorModel": "../host.xml",
    "weekSelectorEnableBeat": true,
    "healthbarColor": "0xB7D855",
    "healthIconsModel": "icon-pico.xml",

    "actionsApplyChartSpeed": false,
    "singAlternateSuffix": null,
    "continuousIdle": false,
    "offsetX": 0,
    "offsetY": 0,
    "alignVertical": "end",
    "alignHorizontal": "start",
    "referenceSize": { "width": -1, "height": -1 },
    "leftFacing": true, 
    "oppositeDirections": {
        "from": ["left", "right"],
        "to": ["right", "left"]
    },

    "actions": {

        "sing": [
            { "direction": "left",   "anim": "Pico NOTE LEFT",  "offsetX": -82 },
            { "direction": "down",   "anim": "Pico Down Note",  "offsetX": -96 },
            { "direction": "up",     "anim": "pico Up note",    "offsetX": -24 },
            { "direction": "right",  "anim": "Pico Note Right", "offsetX": 44  }
        ],

        "miss": [
            { "direction": "left",   "anim": "Pico NOTE LEFT miss",     "offsetX": -82                  },
            { "direction": "down",   "anim": "Pico Down Note MISS",     "offsetX": -102, "offsetY": -48 },
            { "direction": "up",     "anim": "pico Up note miss",       "offsetX": -22                  },
            { "direction": "right",  "anim": "Pico Note Right Miss",    "offsetX": 34                   }
        ],

        "idle": { "anim": "Pico Idle Dance" }

    }
}

